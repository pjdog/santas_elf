{
    "source": {
        "include": ["./src/routes", "./src/utils", "./src/config"],
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        },
        "systemName": "Santa's Elf API Documentation",
        "navType": "vertical",
        "theme": "united",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": false
    }
}
